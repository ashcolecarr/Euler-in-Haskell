-- Project Euler Problem 33

import Data.Time (getCurrentTime, diffUTCTime)

main = do
  start <- getCurrentTime
  putStr $ "The product of the four non-trivial curious fractions is "
  putStrLn $ show (pandigits permutationList [] 0) ++ "."
  stop <- getCurrentTime
  putStrLn $ "Program execution took " ++ show (diffUTCTime stop start) ++ " seconds."

minLimit :: Int
minLimit = 1

maxLimit :: Int
maxLimit = 9

fractions :: [Int]
fractions = [(denominator', nominator) | denominator' <- [minLimit..maxLimit], nominator <- [minLimit..maxLimit], nominator `div` denominator /= 1, nominator `mod` 10 /= 0 && denominator' `mod` 10 /= 0]